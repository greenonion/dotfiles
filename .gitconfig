[user]
	name = Nikos Fertakis
	email = nikos.fertakis@gmail.com
[core]
	editor = emacsclient -a=\"\"
[alias]
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	ci = commit
	co = checkout
	g = grep
[push]
	default = simple
[grep]
	lineNumber = true:
[rebase]
	autosquash = true
[fetch]
	prune = true
[status]
	showUntrackedFiles = all
